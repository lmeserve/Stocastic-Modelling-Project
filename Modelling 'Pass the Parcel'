    "def matrix_creator():\n",
    "    l = 10\n",
    "    u = np.array([0, 1, 3, 2, 1])\n",
    "\n",
    "    M = np.zeros((5, 5))\n",
    "    for item in range(4):\n",
    "        M[item, item] = - (10 + u[item])\n",
    "        M[4, 4] = - u[4]\n",
    "        M[item, item + 1] = u[item + 1]\n",
    "        M[item, item - 1] = l\n",
    "        M[4, 3] = l\n",
    "        M[0, 4] = 0\n",
    "\n",
    "    return M\n",
    "\n",
    "M = matrix_creator()\n",
    "M"
